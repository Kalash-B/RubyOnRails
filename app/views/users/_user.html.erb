<div id="<%= dom_id user %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= user.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Password:</strong>
    <%= user.password %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Mobile number:</strong>
    <%= user.mobile_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Gender:</strong>
    <%= user.gender %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= user.email %>
  </div>
</div>
